<script type="text/javascript">
function functions(){
var s=document.getElementById("search").value;
if(s=="c")
{
window.open("csk.html");
}
else if(s=="m")
{
window.open("matches.html");
}
else if(s=="news")
{
window.open("news.html");
}
else if(s=="h")
{
window.open("highlights.html");
}
else
{
alert("Sorry! This is not here!");
}
}
</script>
